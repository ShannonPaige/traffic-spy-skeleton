<div class="panel-body" id="responseTime">
    <div class="table-responsive">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Response Time</th>
                    <th>#</th>
                </tr>
            </thead>
            <tbody>
                <% Payload.find_one(@user.identifier, :respondedIn).each do |time| %>
                <tr>
                    <td><%= time[0] %></td>
                    <td><%= time[1] %></td>
                </tr>
                <% end %>
            </tbody>
        </table>
    </div>
    <!-- /.table-responsive -->
</div>
<!-- /.panel-body -->

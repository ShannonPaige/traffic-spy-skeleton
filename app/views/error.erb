<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">ERROR</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->


<div id= "error">
  <ul>
  <% if @user.nil? %>
    <li> Error: No user given.</li>
    <% elsif @payload.eventName.nil?%>
    <li> Error: No given Event Name in payload.</li>
    <% else @payload.select(:eventName).empty?%>
    <li> Your event name was not found </li>
    <a href = "/sources/<%= @user.identifier %>/events"/> index of events </a>
  <% end %>
  </ul>
</div>
